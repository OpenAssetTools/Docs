{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://openassettools.dev/schema/font-icon.v1.json",
  "title": "OpenAssetTools font icon json schema",
  "type": "object",
  "required": ["_game", "_type", "_version"],
  "properties": {
    "_game": {
      "enum": ["t6"]
    },
    "_type": {
      "const": "font-icon"
    },
    "_version": {
      "type": "integer",
      "minimum": 1
    }
  },
  "oneOf": [{ "$ref": "http://openassettools.dev/schema/font-icon/t6.v1.json" }]
}
